{
    "env": {
        "development": {
            "presets": [
                ["next/babel", {
                    "styled-jsx": {
                        "plugins": ["styled-jsx-plugin-postcss"]
                    }
                }],
                "@zeit/next-typescript/babel"
            ],
            "plugins": [
                [
                    "module-resolver",
                    {
                        "alias": {"@app": "./"}
                    }
                ],
                "inline-dotenv",
                "transform-inline-environment-variables",
                "inline-react-svg"
            ]
        },
        "production": {
            "presets": [
                ["next/babel", {
                    "styled-jsx": {
                        "plugins": ["styled-jsx-plugin-postcss"]
                    }
                }],
                "@zeit/next-typescript/babel"
            ],
            "plugins": [
                [
                    "module-resolver",
                    {
                        "alias": {
                            "@app": "./"
                        }
                    }
                ],
                "inline-dotenv",
                "transform-inline-environment-variables",
                "inline-react-svg"
            ]
        }
    }
}
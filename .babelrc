{
    "env": {
        "development": {
            "presets": [
                ["next/babel"]
            ],
            "plugins": [
                "inline-dotenv",
                "transform-inline-environment-variables",
                "inline-react-svg",
                [
                    "styled-components",
                    {
                      "ssr": true,
                      "displayName": true,
                      "preprocess": false
                    }
                ]
            ]
        },
        "production": {
            "presets": [
                ["next/babel"]
            ],
            "plugins": [
                "inline-dotenv",
                "transform-inline-environment-variables",
                "inline-react-svg",
                [
                    "styled-components",
                    {
                      "ssr": true,
                      "displayName": false,
                      "preprocess": false
                    }
                ]
            ]
        }
    }
}
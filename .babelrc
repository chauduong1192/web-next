{
    "env": {
        "development": {
            "presets": [
                ["next/babel"],
            ],
            "plugins": [
                "inline-dotenv",
                "transform-inline-environment-variables",
                "inline-react-svg",
                "@babel/plugin-proposal-class-properties",
                "@babel/plugin-proposal-private-methods",
            ]
        },
        "production": {
            "presets": [
                ["next/babel"],
            ],
            "plugins": [
                "inline-dotenv",
                "transform-inline-environment-variables",
                "inline-react-svg"
            ]
        }
    }
}
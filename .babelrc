{
    "env": {
        "development": {
            "presets": [
                "next/babel",
                "@zeit/next-typescript/babel"
            ],
            "plugins": [
                [
                    "module-resolver",
                    {
                        "alias": {"@app": "./"}
                    }
                ],
                "inline-dotenv",
                "transform-inline-environment-variables"
            ]
        },
        "production": {
            "presets": [
                "next/babel",
                "@zeit/next-typescript/babel"
            ],
            "plugins": [
                [
                    "module-resolver",
                    {
                        "alias": {
                            "@app": "./"
                        }
                    }
                ],
                "inline-dotenv",
                "transform-inline-environment-variables"
            ]
        }
    }
}
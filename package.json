{
  "name": "web-next",
  "version": "0.0.2",
  "author": "chau duong",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "NODE_ENV=development ts-node --project tsconfig.server.json server/index.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node .next/server/index.js",
    "prod": "yarn build && yarn start",
    "lint": "tslint -p . -c tslint.json",
    "lint-fix": "tslint -p . -c tslint.json --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest -u --coverage",
    "gen": "plop",
    "gen:single": "plop component single",
    "gen:many": "plop component many",
    "gen:redux": "plop redux",
    "storybook": "NODE_PATH=./ start-storybook -p 3001 -c .storybook -s ./"
  },
  "dependencies": {
    "@types/node": "^14.0.13",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-plugin-inline-dotenv": "^1.2.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "bootstrap": "^4.5.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.4",
    "cross-env": "^7.0.2",
    "dotenv": "^6.2.0",
    "es6-promise": "^4.2.6",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "i18next": "^15.0.4",
    "isomorphic-fetch": "^2.2.1",
    "lru-cache": "^5.1.1",
    "next": "^9.4.4",
    "next-compose-plugins": "^2.1.1",
    "next-i18next": "^4.4.2",
    "next-offline": "^3.3.5",
    "nprogress": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^10.1.2",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.1",
    "webpack": "^4.29.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "@types/styled-jsx": "^2.2.8",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-react-app": "^9.1.2",
    "copy-webpack-plugin": "^4.6.0",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "husky": "^1.3.1",
    "inquirer-directory": "^2.2.0",
    "jest": "^26.0.1",
    "plop": "^2.7.1",
    "plop-pack-fancy-comments": "^0.2.1",
    "redux-mock-store": "^1.5.4",
    "redux-testkit": "^1.0.6",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.1.0",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.9.5",
    "webpack-pwa-manifest": "^4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "license": "ISC"
}

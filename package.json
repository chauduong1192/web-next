    "babel-plugin-inline-react-svg": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "cz-conventional-changelog": "2.1.0",
    "husky": "^1.3.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.12.1",

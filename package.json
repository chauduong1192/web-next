    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.4",
    "cross-env": "^7.0.2",
    "dotenv": "^6.2.0",
    "es6-promise": "^4.2.6",
    "express": "^4.17.1",
    "i18next": "^15.0.4",
    "isomorphic-fetch": "^2.2.1",
    "lru-cache": "^5.1.1",
    "next": "^9.4.4",
    "next-compose-plugins": "^2.1.1",
    "next-i18next": "^4.4.2",
    "next-offline": "^3.3.5",

{
  "name": "web-next",
  "version": "0.1.1",
  "author": "chau duong",
  "private": true,
  "engines": {
    "node": ">=10.0.0 <11"
  },
  "scripts": {
    "dev": "NODE_ENV=development ts-node --project tsconfig.server.json server/index.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node .next/server/index.js",
    "lint": "tslint -p . -c tslint.json",
    "lint-fix": "tslint -p . -c tslint.json --fix",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@types/next": "^8.0.1",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-inline-dotenv": "^1.2.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "i18next": "^15.0.4",
    "lru-cache": "^5.1.1",
    "next": "^8.0.0",
    "next-compose-plugins": "^2.1.1",
    "next-i18next": "^0.32.0",
    "next-offline": "^3.3.5",
    "postcss-nested": "^4.1.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-i18next": "^10.1.2",
    "styled-jsx-plugin-postcss": "^2.0.0",
    "webpack": "^4.29.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "@types/styled-jsx": "^2.2.8",
    "autoprefixer": "^9.4.7",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "2.1.0",
    "husky": "^1.3.1",
    "postcss-easy-import": "^3.0.0",
    "tailwindcss": "^0.7.4",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.3",
    "webpack-pwa-manifest": "^4.0.0"
  },
  "resolutions": {
    "@babel/core": "^7.3.3",
    "styled-jsx": "^3.0.0",
    "webpack": "^4.28.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
